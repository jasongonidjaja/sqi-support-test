{
  "name": "sqi-support-backend",
  "version": "1.0.0",
  "description": "Backend API for SQI Support System (Express + Sequelize + MySQL)",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "NODE_ENV=production node src/server.js",
    "lint": "eslint .",
    "db:drop": "npx sequelize-cli db:drop --config config/config.cjs",
    "db:create": "npx sequelize-cli db:create --config config/config.cjs",
    "db:migrate": "npx sequelize-cli db:migrate --config config/config.cjs",
    "db:seed": "npx sequelize-cli db:seed:all --config config/config.cjs",
    "db:reset": "node cli/db-reset.cjs"
  },
  "keywords": [
    "express",
    "sequelize",
    "mysql",
    "api",
    "backend",
    "sqi-support"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "mysql2": "^3.15.3",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "eslint": "^9.12.0",
    "nodemon": "^3.1.10",
    "sequelize-cli": "^6.6.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
